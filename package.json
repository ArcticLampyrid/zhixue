{
    "name": "zhixue",
    "version": "1.3.1",
    "main": "./dist/server/main.js",
    "scripts": {
        "build": "tsc",
        "start": "npm run build && electron ./dist/server/main.js",
        "dist": "npm run build && electron-builder --win --linux --ia32 --x64 --publish never",
        "publish": "npm run build && electron-builder --win --linux --ia32 --x64 --publish always"
    },
    "author": {
        "name": "qiqiworld",
        "email": "1354092549@qq.com"
    },
    "build": {
        "files": [
            "!src/**/*.ts"
        ],
        "appId": "tk.qiqiworld.zhixue",
        "productName": "智学网",
        "directories": {
            "output": "OutApp"
        },
        "nsis": {
            "language": 2052
        },
        "win": {
            "target": [
                "nsis"
            ]
        },
        "linux": {
            "target": [
                "AppImage"
            ]
        },
        "publish": [
            "github"
        ]
    },
    "repository": "https://github.com/1354092549/zhixue",
    "license": "MIT",
    "keywords": [
        "zhixue"
    ],
    "dependencies": {
        "bootstrap": "^4.6.0",
        "chart.js": "^2.9.4",
        "chartjs-plugin-datalabels": "^1.0.0",
        "custom-electron-titlebar": "^3.2.7",
        "electron-updater": "^4.3.9",
        "https-proxy-agent": "^5.0.0",
        "jquery": "^3.6.0",
        "node-fetch": "^2.6.7",
        "sugar": "^2.0.6",
        "uuid": "^8.3.2"
    },
    "devDependencies": {
        "@types/bootstrap": "^4.6.0",
        "@types/chart.js": "^2.9.32",
        "@types/jquery": "^3.5.5",
        "@types/node": "14.17.3",
        "@types/node-fetch": "^2.5.10",
        "@types/uuid": "^8.3.0",
        "electron": "^12.0.11",
        "electron-builder": "^22.11.7",
        "typescript": "^4.3.4"
    }
}
